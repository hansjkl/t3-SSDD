{
    "VALIDATION": "forward",
    "SERVERS": [
        "R1",
        "R2",
        "R3"
    ],
    // Información inicial de la base de datos
    "DATA": {
        "X": "0"
    },
    "TRANSACTIONS": [
        "T1;BEGIN",
        "T1;WRITE;X,T1",
        // R1 y R2 aceptan T1
        "T1;CAN_COMMIT;R1",
        "T1;CAN_COMMIT;R2",

        // Empieza T2
        "T2;BEGIN",
        "T2;READ;X",
        // R1 y R2 rechazan T2 porque quiere leer X que ya está reservado
        "T2;CAN_COMMIT;R1",
        "T2;CAN_COMMIT;R2",
        // T2 intenta con R3 y ahí es aceptado su CAN_COMMIT
        "T2;CAN_COMMIT;R3",
        
        // T1 hace commit y modifica finalmente X en la base de datos
        "T1;COMMIT",
        // R3 se entera de esto y fuerza a T2 a ser abortada.
        "T2;WRITE;X,HOLIWIS" // No tiene efecto porque T2 ya finalizó
    ]
}